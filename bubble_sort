#include <iostream>
using namespace std;

void sort_func(double a[], int a_size)
{
	for(int i = 0; i < a_size; ++i)
	{
		for(int j = i + 1; j < a_size; ++j)
		{
			if(a[i] > a[j])
			{
				double temp = a[i];
				a[i] = a[j];
				a[j] = temp;
			}
		}
	}
}

int main()
{
  double a[10] = {1,32.3,3.51,3.54,2,64,7.3,9,36,10};

  sort_func(a, 10);

  for(int i = 0; i < 10; ++i)
  {
    cout << "i = " << i << "  a[i] = " << a[i] << endl;
  }
  return 0;
}
